{
    "name": "neoglez/batch-manager",
    "description": "The BatchManager is an attempt to port the Drupal Batch API to the Zend Framework 2 event-driven, service-oriented arquitecture.",
    "type": "library",
    "license": "GPL-2.0+",
    "keywords": [
        "zf3"
    ],
    "authors": [
        {
            "name": "Yansel Gonzalez Tejeda",
            "email": "neoglez@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BatchManager\\": "src/BatchManager"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-config": "3.x",
        "zendframework/zend-crypt": "3.x",
        "zendframework/zend-db": "3.x",
        "zendframework/zend-eventmanager": "3.x",
        "zendframework/zend-http": "3.x",
        "zendframework/zend-i18n": "3.x",
        "zendframework/zend-json": "3.x",
        "zendframework/zend-loader": "3.x",
        "zendframework/zend-modulemanager": "3.x",
        "zendframework/zend-mvc": "3.x",
        "zendframework/zend-serializer": "3.x",
        "zendframework/zend-servicemanager": "3.x",
        "zendframework/zend-session": "3.x",
        "zendframework/zend-stdlib": "3.x",
        "zendframework/zend-uri": "3.x",
        "zendframework/zend-validator": "3.x",
        "zendframework/zend-view": "3.x"
    },
    "require-dev": {
        "phpunit/phpunit": "5.x",
        "squizlabs/php_codesniffer" : "3.*"
    }
}
